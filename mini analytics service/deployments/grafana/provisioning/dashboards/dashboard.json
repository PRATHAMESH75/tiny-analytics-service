{
  "title": "Tiny Analytics Overview",
  "uid": "tiny-analytics",
  "version": 1,
  "schemaVersion": 38,
  "refresh": "10s",
  "timezone": "browser",
  "panels": [
    {
      "type": "stat",
      "title": "Ingest RPS",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus-ds" },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{service=\"ingest_api\"}[5m]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Ingest 5xx Rate",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus-ds" },
      "targets": [
        {
          "expr": "sum(rate(http_errors_total{service=\"ingest_api\"}[5m]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Ingest p95 Latency",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus-ds" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{service=\"ingest_api\"}[5m])) by (le))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Consumer Throughput",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus-ds" },
      "targets": [
        {
          "expr": "rate(enricher_msgs_consumed_total[5m])",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Loader Insert Duration",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "datasource": { "type": "prometheus", "uid": "prometheus-ds" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(loader_insert_duration_seconds_bucket[5m])) by (le))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Daily Pageviews (ClickHouse)",
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 12 },
      "datasource": { "type": "grafana-clickhouse-datasource", "uid": "clickhouse-ds" },
      "targets": [
        {
          "refId": "A",
          "query": "SELECT event_date AS t, count() AS pageviews FROM events GROUP BY event_date ORDER BY event_date ASC",
          "format": "time_series"
        }
      ]
    }
  ]
}
